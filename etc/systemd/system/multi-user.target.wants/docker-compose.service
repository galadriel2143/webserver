After=dockermailer.service dockervpn.service
Requires=dockermailer.service dockervpn.service

[Service]
Type=idle
ExecStart=/usr/bin/docker-compose -f /home/docker/www/docker-compose.yml up
KillMode=process
Environment="DOMAIN_NAME=msgor.com"

[Install]
